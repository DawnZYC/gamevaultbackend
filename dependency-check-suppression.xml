<?xml version="1.0" encoding="UTF-8"?>
<!--
  OWASP Dependency Check Suppression File
  Use this file to suppress false positives
  Based on Workshop 8 - SAST
-->
<suppressions xmlns="https://jeremylong.github.io/DependencyCheck/dependency-suppression.1.3.xsd">

    <!-- Example: Suppress specific CVE for a library -->
    <!--
    <suppress>
        <notes><![CDATA[
            False positive: This vulnerability doesn't affect our usage
        ]]></notes>
        <cve>CVE-2023-XXXXX</cve>
    </suppress>
    -->

    <!-- Example: Suppress by file path pattern -->
    <!--
    <suppress>
        <notes><![CDATA[
            Test dependencies are not included in production
        ]]></notes>
        <filePath regex="true">.*[\\/]test[\\/].*</filePath>
    </suppress>
    -->

    <!-- Suppress H2 database vulnerabilities (only used in tests) -->
    <suppress>
        <notes><![CDATA[
            H2 database is only used in test scope and not exposed in production
        ]]></notes>
        <gav regex="true">^com\.h2database:h2:.*$</gav>
    </suppress>

</suppressions>
